<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.dalbanchin.savwar">
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconsavwar"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/iconsavwar_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".activities.MyApp">
        <activity android:name=".activities.SavoirDuJour"></activity>
        <activity android:name=".activities.ThemeActivity"></activity>
        <activity android:name=".activities.FavorisActivity"></activity>
        <activity
            android:name=".activities.SplashActivity" android:theme="@style/SplashTheme">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.MainActivity" />
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>